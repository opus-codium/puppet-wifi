[connection]
id=<%= @name %>
uuid=<%= @real_uuid %>
type=wifi

[wifi]
ssid=<%= @name %>
mode=infrastructure
<% if @mac -%>
mac-address=<%= @mac %>
<% end -%>

<% if @psk -%>
[wifi-security]
key-mgmt=wpa-psk
auth-alg=open
psk=<%= @psk %>
<% end -%>

[ipv4]
method=auto

[ipv6]
method=auto
ip6-privacy=2
