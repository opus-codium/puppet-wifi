[connection]
id=<%= $name %>
uuid=<%= $real_uuid %>
type=wifi

[wifi]
ssid=<%= $name %>
<% if $bssid { -%>
bssid=<%= $bssid %>
<% } -%>
mode=infrastructure
<% if $mac { -%>
mac-address=<%= $mac %>
<% } -%>

<% if $real_psk { -%>
[wifi-security]
key-mgmt=wpa-psk
auth-alg=open
psk=<%= $real_psk %>
<% } -%>

[ipv4]
method=auto
<% if $dns.size > 0 { -%>
dns=<%= $dns.map |$server| { "${server};" }.join %>
ignore-auto-dns=true
<% } -%>

[ipv6]
method=auto
ip6-privacy=2
